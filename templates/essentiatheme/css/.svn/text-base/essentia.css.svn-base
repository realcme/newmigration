#nav { font-size: 1em;font-family:  Arial,​Helvetica,​sans-serif; margin-left: 8px}
#nav a[title=sub] { }
#nav:after{ content: "."; display:block; height:0; font-size:0; clear:both; visibility:visible; }
#nav ul { margin:0px; padding:0px; list-style:none; text-align: center; }
#nav ul li { width: 188px; background: url(../img/nav_bg.jpg);background-repeat: repeat-x; margin:0px 2px 0px 0px; padding:8px 0px; list-style:none; float:left;; position: relative }
#nav ul li a {  padding:8px; margin:0px; color:#FFF; text-decoration:none; font-weight:normal; }
#nav ul li:hover,#nav ul li.selected { width: 188px; background: url(../img/nav_bg_over.jpg);background-repeat: repeat-x; margin:0px 2px 0px 0px; padding:8px 0px; list-style:none; float:left; }
#nav ul li a:hover { color:#FFF; text-decoration:none; }

/* 1ST LEVEL DROP-DOWN MENU */
#nav ul li ul { display:none; }
#nav ul li:hover > ul { float:left; display:block;  position: absolute; left: 0; top: 30px;  z-index: 1000}
#nav ul li:hover > ul li, #nav ul li.selected > ul li  { 
	 height: 21px; float:left; list-style:none; background: url(../img/blue_table_bg.png); 
	 margin-right: 0;
   padding-left: 15px;
   padding-top: 8px;
   text-align: left;
   text-decoration: none;
   vertical-align: bottom;
   white-space: nowrap;
   width: 187px !important;
   border-bottom: 1px solid #BBF0FF;
}
#nav ul li:hover > ul li a, #nav ul li.selected > ul li a { text-decoration:none; color: #00B2E2; font-weight:normal;}
#nav ul li:hover > ul li a:hover { }

/* 2ND LEVEL DROP-DOWN MENU */
#nav ul li > ul li ul { display:none; }
#nav ul li > ul li:hover > ul { float:left;display:block; }
#nav ul li > ul li ul li, #nav ul li.selected > ul li ul li, #nav ul li > ul li:hover ul li, #nav ul li.selected > ul li:hover ul li { 
   height: 21px; float:left; list-style:none; background: url(../img/blue_table_bg.png); 
   margin-left: 187px; 
   margin-right: 0;
   padding-left: 15px;
   padding-top: 8px;
   text-align: left;
   text-decoration: none;
   vertical-align: bottom;
   white-space: nowrap;
   width: 187px !important;
   border-bottom: 1px solid #BBF0FF;
}
#nav ul li > ul li:hover ul li a { font-weight:normal; }
#nav ul li > ul li:hover ul li a:hover { }

