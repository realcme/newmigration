.smdc_footer
{
	color:#585858;
	font-family:Arial, Helvetica, sans-serif;
	font-size:11px;
	font-weight:bold;
	text-align:right;
	padding-right:10px;
	letter-spacing:1px;
}

.smdc_footer table
{
	margin: 0px;
}
.smdc_footer a:link {
	color:#0839AE;
	text-decoration:none;
}
.smdc_footer a:active {
	color:#0839AE;
	text-decoration:none;
}
.smdc_footer a:visited {
	color:#0839AE;
	text-decoration:none;
}
