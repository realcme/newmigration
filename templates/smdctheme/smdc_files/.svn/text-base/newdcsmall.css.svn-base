	background-color:#87afb5;
	height:64px;
	font-family: arial, helvetica, sans-serif;
}
#searchField
{
	position:absolute; 
	width:154px; 
	height:16px; 
	background-image:url(Images/searchArea160by19.gif); 
	padding-top:3px; 
