/*
 * rcm_reset.css
 */

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,p,blockquote,th,td,select{font-family: verdana,sans-serif;font-size:100%;background-color: transparent;border-color: transparent;margin:0;padding:0;}
textarea, input {font-family: verdana,sans-serif;font-size:100%;margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset,img{border:0;}
address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}
ol,ul {list-style:none;}
caption,th {text-align:left;}
h1,h2,h3,h4,h5,h6{font-family: verdana,sans-serif;font-size:100%;background-color: transparent;border: none;}
q:before,q:after{content:'';}
abbr,acronym {border:0;}
strong{font-weight:bold;}
em{font-style:italic;}
.forumpost {margin-top:0px;background-color: #F6F6F6;}
.forumheaderlist, .forumpost {border: none;}
.forumpost .content {background-color: #F6F6F6;padding:5px;}
.forumpost .left {background-color:#FFFFFF;padding: 0px;text-align: left;width: 99.6%;}
a:link, a:visited, a:active, a:hover {text-decoration:underline;}
 
/*
 * rcm_fonts.css
 */

/**
 * Percents could work for IE, but for backCompat purposes, we are using keywords.
 * x-small is for IE6/7 quirks mode.
 * 
 */
body {font:12px arial,helvetica,clean,sans-serif;*font-size:small;*font:x-small;}
table {font-size:inherit;font:100%;}

/**
 * 99% for safari; 100% is too large
 */
select, input, textarea {font:99% arial,helvetica,clean,sans-serif;}

/**
 * Bump up !IE to get to 13px equivalent
 */
pre, code {font:115% monospace;*font-size:100%;}

/**
 * Default line-height based on font-size rather than "computed-value"
 * see: http://www.w3.org/TR/CSS21/visudet.html#line-height
 */
body * {line-height:1.5em;}

 
