#menu {
	margin: -4px 0 0 19px;
}
*html #menu {
	margin: -4px 0 0 24px;
}
*+html #menu {
	margin: -4px 0 0 24px;
}
#menu li {
	MARGIN: 0px 2px;
	padding: 0 0 8px;
	float: left;
	list-style: none;
	position: relative;	
}


/* main level link */
#menu a {
	font-weight: normal;
	color: #ffffff;
	text-decoration: none;
	text-transform:uppercase;
	FONT-SIZE: 10px; 
	PADDING-LEFT:10px; 
	margin:0;
	BACKGROUND: url("./img/nav_left.gif") no-repeat left top; 	
}

#menu A SPAN {
	PADDING-RIGHT: 15px; 
	background:url("./img/nav_right.gif") no-repeat right top;
	PADDING-BOTTOM: 9px; 
	PADDING-TOP: 7px
}
#menu A {
	DISPLAY: block; FLOAT: left
}

#menu A SPAN {
	DISPLAY: block; FLOAT: left; color:#3B3B3B;
}

#menu A {
	FLOAT: none
}
#menu A SPAN {
	FLOAT: none
}
#menu A:hover {
	PADDING-LEFT: 10px; COLOR: #E4261B
}

#menu A:hover SPAN {
	PADDING-RIGHT: 15px; color:#E4261B;
}
#menu A.slected {
	PADDING-LEFT: 15px; COLOR: #E4261B
}
#menu A.slected SPAN {
	PADDING-RIGHT: 12px; 
}


/* main level link hover */
#menu .current a, #menu li:hover > a {
	color: #444;
}

/* sub levels link hover */
#nav ul li:hover a, #menu li:hover li a {
	border-bottom:1px solid #aaaaaa;
	padding:7px 5px 7px 18px;
	background:url("./img/arrow.png") 0 5px no-repeat #fff;
	color: #666;
	margin:1px 4px 0 4px;
}
#nav ul li:hover a, #menu li:hover li a.last {
border:none;
}
#menu ul a:hover {
	color: #e4261b !important;
	background:url("./img/arrow.png") 0 5px no-repeat #f8eeee !important;
}

/* dropdown */
#menu li:hover > ul {
	display: block;
}

/* level 2 list */
#menu ul {
	display: none;
	margin: 0;
	padding: 0;
	width: 154px;
	position: absolute;
	float:left;
	top: 30px;
	left: 0px;
	background:url("./img/menu_bg.png") 0 0 no-repeat;
}
#menu ul li {
	float: none;
	margin: 0;
	padding: 0;
}

#menu ul a {
	font-weight: normal;
}

/* level 3+ list */
#menu ul ul {
	left: 152px;
	top: -1px;
}

/* rounded corners of first and last link */
#menu ul li:first-child > a {
}
#menu ul li:last-child > a {
}

/* clearfix */
#menu:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
#menu {
	display: inline-block;
} 
html[xmlns] #menu {
	display: block;
	float:left;
}
 
* html #menu {
	height: 1%;
}

.header_table td {
	padding: 0;
}
