@CHARSET "ISO-8859-1";

body {
  background-color:#fff;
}
#page
{
	/* recommended styles */
	margin: auto; /* marker :m1: delete this to make layout left aligned */

	/* add custom styles here */
	width: 780px; /* marker :m3:  */
	/*border: 1px solid #000;*/
}
#logo /* Wrapper for logo */
  { float: left; margin: 15px 0 0; padding: 0;}
#logo-partner /* Wrapper for partner logo */
  { float: right; margin: 15px 0 0; padding: 0;}  

#content
  { margin: 0; padding: 0; }
/*HEADER*/
#content-header /* Wrapper for breadcrumb, title, messages, tabs, and help */
  {
	position: relative;
	width: 780px;
	height: 32px;
	z-index: 10;
	margin: 0px;
	padding: 0;
	zoom: 1; }

#content-inner > .tabs { clear: both; }

.with-navbar #content,
.with-navbar #sidebar-left,
.with-navbar #sidebar-right
{
    margin-top: 10px; 
}
.authoring-home-title { background-color: transparent; background-image: url(../img/authoring_home_title.gif); background-repeat: no-repeat; width: 250px; height: 23px; margin: 0; padding: 0; }

#page-title {font-size: 20px;color: #E57E1C;font-weight: normal;font-family:arial,verdana,sans-serif;float: left;}
#authoring-header-default {}
#btn-activities-home { background-color: transparent; background-image: url(../img/cme_activities_home.gif);background-repeat: no-repeat; width: 137px; height: 23px; margin: 2px 6px 7px 0; padding: 0;float: right; display: inline;}
#btn-activities-home:hover { background-color: transparent; background-image: url(../img/cme_activities_home_mo.gif); background-repeat: no-repeat; }
#btn-activities-home span { display: none; }
#btn-authoring-tool-home { background-color: transparent; background-image: url(../img/authoring_tool_home.gif); background-repeat: no-repeat; width: 139px; height: 25px; margin: 0 4px 0 0; padding: 0; float: right;  display: inline; }
#btn-authoring-tool-home:hover { background-color: transparent; background-image: url(../img/authoring_tool_home_mo.gif); background-repeat: no-repeat; }
#btn-authoring-tool-home span { display: none; }
#btn-course-templates { background-color: transparent; background-image: url(../img/course_templates.gif); background-repeat: no-repeat; width: 139px; height: 25px; margin: 0 4px 0 0; padding: 0; float: right;  display: inline; }
#btn-course-templates:hover { background-color: transparent; background-image: url(../img/course_templates_mo.gif); background-repeat: no-repeat; }
#btn-course-templates span { display: none; }
#authoring-toolkit-default {display: inline; float: right; margin-right: 2px }

#btn-template-builder { background-color: transparent; background-image: url(../img/cme_template_builder.gif);background-repeat: no-repeat; width: 36px; height: 32px; margin: 0px 5px 7px 0; padding: 0;float: right; display: inline;}
#btn-template-builder:hover { background-color: transparent; background-image: url(../img/cme_template_builder_mo.gif); background-repeat: no-repeat; }
#btn-template-builder span { display: none; }

#btn-dashboard { background-color: transparent; background-image: url(../img/cme_dashboard.png);background-repeat: no-repeat; width: 32px; height: 32px; margin: 0px 5px 7px 0; padding: 0; float: left; display: inline;}
#btn-dashboard span { display: none; }

/*AUTHORING TOOLKIT DROPDOWN*/
#toolkit { clear: right; list-style-type: none; margin: 0; padding: 0; }
#toolkit ul { list-style-type: none; margin: 0; padding: 0; }
#toolkit a {
	text-decoration: none;
	width: 164px;
	display: block; }
#toolkit li { position: relative; width: 164px; height: 45px; float: left;}
#toolkit li a { display: block; height: 30px; width: 119px; font-weight: normal; font-size: 10px;color: rgb(102, 102, 102); border: 1px solid grey; padding-left: 45px; background-color: #FFFFFF }
#toolkit li a:hover {background-color: #E8E8E8; color: #404040  }
#toolkit li ul { background-color: transparent; background-image: url(../img/authoring_toolkit_bg.gif); background-repeat: no-repeat; position: absolute; left: -999em; padding-top: 26px; }
#toolkit li:hover ul, #toolkit li.over ul, #toolkit li.sfhover ul { background-color: transparent; background-image: url(../img/authoring_toolkit_bg.gif); background-repeat: no-repeat; position: absolute; top: 2px; left: 0; width: 164px; height: 26px; display: block; }
#toolkit li a.top { background-color: transparent; background-image: url(../img/authoring_toolkit_top.gif); background-repeat: no-repeat; width: 164px; height: 26px; margin: 2px 0 0; padding: 0; border: 0}
#toolkit li a:hover.top {
  background-color: transparent;
  background-image: url(../img/authoring_toolkit_bg.gif);
  background-repeat: no-repeat; }
#toolkit li li a.curriculum { background-image: url(../img/curriculum_sm.gif); background-repeat: no-repeat; padding-top: 15px;border-top: 0px}
#toolkit li li a.picmelite { background-image: url(../img/picmelite_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.faculty { background-image: url(../img/faculty_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.learning_objectives { background-image: url(../img/learning_objectives_sm.gif); background-repeat: no-repeat; padding-top: 8px; height: 37px; }
#toolkit li li a.quality_measures { background-image: url(../img/quality_measures_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.patients { background-image: url(../img/patients_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.consultants { background-image: url(../img/consultants_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.series { background-image: url(../img/series_sm.gif); background-repeat: no-repeat; padding-top: 8px; height: 37px; }
#toolkit li li a.certificate { background-image: url(../img/certificate_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.surveys { background-image: url(../img/surveys_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.references { background-image: url(../img/references_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.relevant_terms { background-image: url(../img/relevant_terms_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.front_matter { background-image: url(../img/front_matter_sm.gif); background-repeat: no-repeat; padding-top: 8px; height: 37px; }
#toolkit li li a.primary_audience { background-image: url(../img/primary_audience_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.account_management { background-image: url(../img/user_accounts_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.evaluation_questions { background-image: url(../img/evaluation_questions_sm.gif); background-repeat: no-repeat; padding-top: 8px; height: 37px; }
#toolkit li li a.metrics { background-image: url(../img/metrics_sm.gif); background-repeat: no-repeat; padding-top: 15px}
#toolkit li li a.bottom {
	font-size: 0;
	line-height: 0;
	background-color: #b8b8b9;
	margin: 0;
	padding: 0;
	width: 164px;
	height: 3px; }
#toolkit li li a:hover.bottom {
	font-size: 0;
	line-height: 0;
	background-color: #b8b8b9;
	margin: 0;
	padding: 0;
	width: 164px;
	height: 3px; }
/*PAGE LAYOUT*/
#author-default-top {
	font-size: 0;
	line-height: 0;
	background-color: transparent;
	background-image: url(../img/author_default_top.gif);
	background-repeat: no-repeat;
	width: 780px;
	height: 6px;
	margin: 0;
	padding: 0; 
	clear: both; }
#content-area { background-color: transparent; background-image: url(../img/author_default_content_bgtile.gif); background-repeat: repeat-y; width: 771px; margin: 0; padding: 8px 10px 10px 4px; clear: both; }
#author-default-bottom { background-color: transparent; background-image: url(../img/author_default_bottom.gif); background-repeat: no-repeat; width: 780px; height: 8px; margin: 0; padding: 0; }
/*PAGE CONTENTS*/
.view-authoractivities {
	margin: 0;
	padding: 0; }
.view-authoractivities form {
	margin: 0;
	padding: 0; }
.view-authoractivities table { width: 761px; margin: 0 5px; border-style: none; border-width: 0; }
.view-authoractivities table thead {
	color: #535353;
	background-color: #ddd;
	margin: 0;
	padding: 0;
	border-style: none;
	border-width: 0; }
.view-authoractivities table thead th {
	color: #535353;
	background-color: #ddd;
	text-align: left;
	margin: 0;
	padding-left: 10px;
	border-style: none;
	border-width: 0; }
.view-authoractivities table tbody tr.odd td { background-color: #ddd; padding-bottom: 10px; border-style: none; border-width: 0; }
.view-authoractivities table tbody input.form-text { margin-right: 10px; margin-left: 10px; width: 95%; }
.view-authoractivities table tbody input.form-submit {
	font-size: 0;
	background-color: transparent;
	background-image: url(../img/go.gif);
	background-repeat: no-repeat;
	background-position: left 50%;
	text-align: left;
	width: 26px;
	height: 20px;
	margin: 0;
	padding: 0;
	border-style: none; }
.view-authoractivities table tbody input.form-submit:hover {
	font-size: 0;
	background-color: transparent;
	background-image: url(../img/go_mo.gif);
	background-repeat: no-repeat;
