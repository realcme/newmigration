	height: 20px;
	margin: 0;
	padding: 0;
	border-style: none; }
/*MAIN CONTENT TABLE*/
.view-content-authoractivities table { width: 760px; margin: 2px 0 6px 6px; padding: 0; border-color: #ccc #ccc #a3a3a3; border-style: solid; border-width: 1px; }
*+html .view-content-authoractivities table { margin: 1px 0 6px 5px; padding: 0; width: 761px; border-style: solid; border-width: 1px; border-color: #ccc #ccc #a3a3a3; }
* html .view-content-authoractivities table { margin: 1px 0 6px 5px; padding: 0; width: 761px; border-style: solid; border-width: 1px; border-color: #ccc #ccc #a3a3a3; }
.view-content-authoractivities table thead th { background-color: transparent; background-image: url(../img/authoractivities_bgtile.gif); background-repeat: repeat-x; text-align: center; height: 46px; margin: 0 0 0 1px; padding: 0 5px; border-right: 1px solid #bcbbbb; border-top-style: none; border-top-width: 0; border-bottom-style: none; border-bottom-width: 0; border-left-style: none; border-left-width: 0; }
.view-content-authoractivities table tbody tr.odd td { background-color: #e7e7e7; margin: 0; padding: 5px; border-right: 1px solid #aaa9a9; border-bottom: 1px solid #ccc; border-top-style: none; border-top-width: 0; border-left-style: none; border-left-width: 0; }
.view-content-authoractivities tr.odd a:link { color: #d0660a; }
