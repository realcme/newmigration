.view-content-authoractivities table tbody tr.even td { background-color: #f6f6f6; margin: 0; padding: 5px; border-right: 1px solid #aaa9a9; border-bottom: 1px solid #ccc; border-top-style: none; border-top-width: 0; border-left-style: none; border-left-width: 0; }
