/*PAGER*/
.pager  { background-color: transparent; background-image: url(../img/pager_bgtile.gif); background-repeat: repeat-x; text-align: left; height: 50px; margin: 0; padding-top: 10px; padding-left: 5px; position: relative; }
.nopager { background-color: transparent; background-image: url(../img/pager_bgtile.gif); background-repeat: repeat-x; text-align: left; margin: 0px 5px 0px 5px; padding-top: 10px; padding-left: 5px; height: 50px; position: relative; }
.page-activity_templates .nopager { height: 40; }
.pager strong.pager-current { color: #fff; font-weight: bold; line-height: 2em; background-color: #63499b; margin-right: 2px; margin-left: 2px; padding: 3px 8px; border: solid 1px #63499b; }
.pager a, .pager-last  { padding: 3px 8px; color: #63499b; font-weight: bold; line-height: 2em; background-color: #fff; margin-right: 2px; margin-left: 2px; border: solid 1px #eaeaea; }
#author-new-activities { background-color: transparent; background-image: url(../img/create_new_activity.gif); background-repeat: no-repeat; position: absolute; right: 15px; bottom: 12px; width: 136px; height: 22px; display: inline; }
#author-new-activities:hover { background-color: transparent; background-image: url(../img/create_new_activity_mo.gif); background-repeat: no-repeat; }
#author-new-template { background-color: transparent; background-image: url(../img/create_new_template.gif); background-repeat: no-repeat; position: absolute; right: 15px; bottom: 12px; width: 136px; height: 22px; display: inline; }
