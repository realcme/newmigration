.view-content-authoractivities tr.even a:visited { color: #d0660a; }
.view-content-authoractivities tr.even a:hover, a:focus { color: #69c; }
.view-content-authoractivities table tbody tr.published td { background-color: #d6d2e2; margin: 0; padding: 5px; border-right: 1px solid #aaa9a9; border-bottom: 1px solid #ccc; border-top-style: none; border-top-width: 0; border-left-style: none; border-left-width: 0; }
