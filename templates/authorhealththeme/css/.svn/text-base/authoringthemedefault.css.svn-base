div.submit-buttons { text-align: right; padding: 10px 30px 15px 0; }
div.submit-buttons > input[type=submit] {
	border: 1px solid #d47600;
	color: white;
	font-family: Arial;
	font-size: 9pt;
	padding: 1px 8px 2px 8px;
	text-shadow: 2px 2px #7c470a;
  border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
	cursor: pointer;
  margin: 12px 6px 3px 0;
  background: #d47600;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffae44', endColorstr='#d47600');
  background: -webkit-gradient(linear, left top, left bottom, from(#ffae44), to(#d47600));
  background: -moz-linear-gradient(top, #ffae44, #d47600);
}
div.submit-buttons > input[type=submit]:hover {
  background: #ffae44;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffae44', endColorstr='#ffae44');
  background: -webkit-gradient(linear, left top, left bottom, from(#ffae44), to(#ffae44));
  background: -moz-linear-gradient(top, #ffae44, #ffae44);
  border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
}
div.submit-buttons > #cancel, div.submit-buttons > #return {
  border: 1px solid #ab9be0;
  text-shadow: 2px 2px #827f8f;
  background: #7d6eac;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#cdc7e0', endColorstr='#7d6eac');
  background: -webkit-gradient(linear, left top, left bottom, from(#cdc7e0), to(#7d6eac));
  background: -moz-linear-gradient(top, #cdc7e0, #7d6eac);
}
div.submit-buttons > #cancel:hover, div.submit-buttons > #return:hover {
  background: #cdc7e0;
	  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#cdc7e0', endColorstr='#cdc7e0');
  background: -webkit-gradient(linear, left top, left bottom, from(#cdc7e0), to(#cdc7e0));
  background: -moz-linear-gradient(top, #cdc7e0, #cdc7e0);
}

