	margin: 0 5px;
	background-color: #DDDDDD;
	padding: 5px 10px;
	font-family: Arial,verdana,sans-serif;
	}
	
.view-header-authoractivities .title, .view-header-activity-templates .title, .view-header-all-meetings .title {
	text-transform: uppercase;
	font-weight: bold;
	color: #535353;
	margin-bottom: 2px;
	}


/*ADD NEW ACTIVIYTY*/
#create-activity {	margin: 0 5px;	padding: 0;}
#create-activity h1 {	color: #535353;	margin: 0 0 5px;	padding: 0; }
.form-item label {	margin-bottom: 5px; }
.description {	margin-top: 5px;	padding: 0; }
.form-submit-activity input.form-submit {	font-size: 0;	background-color: transparent;	width: 136px;	background-image: url(../img/create_new_activity.gif);	background-repeat: no-repeat;	height: 22px;	margin: 0 0 10px;	padding: 0;	border-style: none; }
.form-submit-activity input.form-submit:hover {	background-image: url(../img/create_new_activity_mo.gif);	background-repeat: no-repeat;	border-style: none; }
