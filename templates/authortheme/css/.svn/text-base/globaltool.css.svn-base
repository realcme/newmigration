#gt-header-icon-user-accounts { background-color: transparent; background-image: url(../img/globaltool/gt_icon_ua.gif); background-repeat: no-repeat; width: 48px; height: 54px; float: left; margin: 0; padding: 0; }
/*Add New Button*/
#gt-addnew-account-button { background-color: transparent; background-image: url(../img/create_new_account.gif); background-repeat: no-repeat; text-align: center; margin: 0 auto; padding: 0; width: 161px; height: 21px; display: block; }
#gt-addnew-account-button:hover { background-color: transparent; background-image: url(../img/create_new_account_mo.gif); background-repeat: no-repeat; text-align: center; width: 161px; margin: 0 auto; padding: 0; height: 21px; display: block; }
#gt-addnew-account-button-disabled { background-color: transparent; background-image: url(../img/create_new_account_dis.gif); background-repeat: no-repeat; text-align: center; margin: 0 auto; padding: 0; width: 161px; height: 21px; display: block; }

#account-cancel { background-image: url(../img/globaltool/cancel.gif); background-color: transparent; background-repeat: no-repeat; height: 22px; width: 59px; float: left; margin: 0; padding: 0; }
#account-cancel:hover { background-color: transparent; background-image: url(../img/globaltool/cancel_mo.gif); background-repeat: no-repeat; margin: 0; padding: 0; width: 59px; height: 22px; float: left; }
#account-save input.form-submit { font-size: 0; background-color: transparent; width: 52px; background-image: url(../img/save.gif); background-repeat: no-repeat; height: 22px; float: left; margin: 0 0 0 5px; border-style: none; }
#account-save input.form-submit :hover { background-image: url(../img/save_mo.gif); background-repeat: no-repeat; border-style: none; }
.account-submit {margin-top: 20px; }

#cmeauthoradmin-partnerform table { width: 100%; margin: 2px 0 6px 6px; padding: 0; border-color: #ccc #ccc #a3a3a3; border-style: solid; border-width: 1px; }
#cmeauthoradmin-partnerform table thead th { color: #000000; background-color: transparent; background-image: url(../img/authoractivities_bgtile.gif); background-repeat: repeat-x; text-align: center; height: 46px; margin: 0 0 0 1px; padding: 0 5px; border-right: 1px solid #bcbbbb; border-top-style: none; border-top-width: 0; border-bottom-style: none; border-bottom-width: 0; border-left-style: none; border-left-width: 0; }
#cmeauthoradmin-partnerform table tbody tr.odd td { background-color: #e7e7e7; margin: 0; padding: 2px; border-right: 1px solid #aaa9a9; border-bottom: 1px solid #ccc; border-top-style: none; border-top-width: 0; border-left-style: none; border-left-width: 0; }
#cmeauthoradmin-partnerform table tbody tr.even td { background-color: #f6f6f6; margin: 0; padding: 2px; border-right: 1px solid #aaa9a9; border-bottom: 1px solid #ccc; border-top-style: none; border-top-width: 0; border-left-style: none; border-left-width: 0; }
#cmeauthoradmin-partnerform td.save  { text-align: center;  }
#cmeauthoradmin-partnerform .form-checkboxes { text-align: center; }
#cmeauthoradmin-partnerform .name {white-space: nowrap;}

#cmeauthoradmin-newaccountform, #account-help {
  margin-top: 20px;
  margin-left: 20px;
}
.global-delete-error {
  background-color: #FFFFFF; 
  font-weight: bold; 
  color: red; 
  border: 1px solid #CCCCCC; 
  margin: 20px 10px; 
  padding-left: 30px;
  padding-top: 10px;
  padding-bottom: 50px;
  background-image: url(../img/warning.png);
  background-position: 5px 5px;
  background-repeat: no-repeat;  
}
#global-delete-cancel { background-image: url(../img/globaltool/cancel.gif); background-color: transparent; background-repeat: no-repeat; height: 22px; width: 59px; float: left; margin: 0; padding: 0; }
#global-delete-cancel:hover { background-color: transparent; background-image: url(../img/globaltool/cancel_mo.gif); background-repeat: no-repeat; margin: 0; padding: 0; width: 59px; height: 22px; float: left; }
/*Search and re-order*/
#gt-views-search { float: left; position:absolute; top:0; left:0; width: 85%; }
#gt-views-search tr.even, tr.odd { border: none; }
#gt-views-search td; { padding-bottom: 0px; }
#gt-views-order { font-family: Arial, verdana, sans-serif; background-color: #EEEEEE; color: #666666; padding-left: 10px; padding-top: 2px; text-align: left; font-weight: bold; position: absolute; top: 0; right: 0; width: 15%; }
#gt-views-order select { margin-top: 5px; }
#gt-views-args { height: 59px; background-color: #EEEEEE; clear: both;position:relative; border-bottom: 1px solid #D5D5D5}
#gt-views-byactivity { height: 59px; background-color: #EEEEEE; clear: both;position:relative; border-bottom: 1px solid #D5D5D5}

.half-width {clear: none;float: left;margin: 8px 0;width: 48%;}

