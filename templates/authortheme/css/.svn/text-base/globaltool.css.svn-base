@CHARSET "ISO-8859-1";


html, body { margin: 0; padding: 0; }
body { background-color:#fff; position: relative; }
.with-navbar #content,
  .with-navbar #sidebar-left,
  .with-navbar #sidebar-right
  { margin: 0; padding: 0; }
.hidefield {
  display: none;
 }
 
/*FCK Editor */
.textarea-identifier, .fckeditor { display: none; }
.fck-div { position: relative; font-family: verdana; border: 2px groove #f0f0f0; border-top: 2px solid white; border-left: 2px ridge #f0f0f0; margin: 0 0 3px 0; background: white; overflow: hidden; }
.fck-div-div { overflow: auto; cursor: text; line-height: 1.2em; }
.fck-div-div ul { display: block !important; list-style: disc outside none !important; margin: 0; -moz-padding-start: 30px !important; }
.fck-div-div ul li { list-style: disc outside none !important; }
.fck-div-div ol { display: block !important; list-style: decimal outside none !important; margin: 0; -moz-padding-start: 30px !important; }
.fck-div-div ol li { list-style: decimal outside none !important; }
.fck-div.red { border: 1px solid red; }
.fck-tooltip { display:none; }
/* .fck-tooltip { display:none; } */
/* .fck-div:hover .fck-tooltip { display: inline; position: absolute; padding: 5px; color: #000; background-color:#FEE; border:1px solid #000; text-decoration: none; } */
.tb-RealCME { padding-top: 50px; background-image: url(../img/tb_realcme.gif); background-repeat: no-repeat;  }
.tb-RealCMEMinimal { padding-top: 26px; background-image: url(../img/tb_realcmeminimal.gif); background-repeat: no-repeat;  }
.tb-RealCMEVariant1 { padding-top: 50px; background-image: url(../img/tb_realcmevariant1.gif); background-repeat: no-repeat;  }
.tb-RealCMEVariant2 { padding-top: 50px; background-image: url(../img/tb_realcmevariant2.gif); background-repeat: no-repeat;  }
.tb-RealCMEVariant3 { padding-top: 50px; background-image: url(../img/tb_realcmevariant3.gif); background-repeat: no-repeat;  }

/* TEMP */
.gt-fck-div { border: 1px inset gray; margin: 0 1px; background: white; overflow-x: auto; overflow-y: hide; }

