#navbar {
	display: none;
}

.breadcrumb {
	display: none;
}

.bold {
	font-weight: bold;
}

.right {
	float: right;
}

.left {
	float: left;
}

.center {
	display: block;
	margin-left: auto;
	margin-right: auto;
}

.cscore {
	display: none !important;
}
.barscore-value {
	font-weight: bold;
  float: left;
  padding: 0px 5px 0px 2px;
}
.barscore-value.cscore {
  padding-right: 7px;
}

.barscore-text {
	font-size: 11px;
	font-family: Arial, verdana, sans-serif;
	font-weight: normal;
	text-align: right;
	position: relative;
	float: left;
	clear: both;
	margin: 0;
	padding: 0 2px 0 0;
	display: inline;
}
.barscore-bar {
  float: left;	
}

.score {
	font-size: 0;
	background-color: transparent;
	background-image: url(../img/score.gif);
	background-repeat: repeat-x;
	background-position: 100% center;
	position: relative;
	height: 12px;
	float: left;
	margin: 0;
	padding: 0;
	border: solid 1px #666;
	display: inline;
}
.barscore-bar.cscore{
	clear: left;
}
.score.cscore  {
  background-image: url(../../images/scoreothers.gif);
}


.score-blank {
	font-size: 0;
	background-color: #f4f4f4;
	position: relative;
	height: 12px;
	float: left;
	margin: 0;
	padding: 0;
	border-top: 1px solid #666;
	border-right: 1px solid #666;
	border-bottom: 1px solid #666;
	display: inline;
}

#healthcourse-records {
	padding: 5px;
  background-color: white;
  -moz-box-shadow: 0px 0px 20px #DDD;
  -webkit-box-shadow: 0px 0px 20px #DDD;;
  box-shadow: 0px 0px 20px #DDD;
}

.healthcourse-record {
	height:80px;
  margin-bottom: 4px;
  padding:4px 4px 6px 4px;
  border: solid thin #dbdbdb;
  background: rgb(238,238,238); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(238,238,238,1) 0%, rgba(226,226,226,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(238,238,238,1)), color-stop(100%,rgba(226,226,226,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(238,238,238,1) 0%,rgba(226,226,226,1) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(238,238,238,1) 0%,rgba(226,226,226,1) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(238,238,238,1) 0%,rgba(226,226,226,1) 100%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(238,238,238,1) 0%,rgba(226,226,226,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eeeeee', endColorstr='#e2e2e2',GradientType=0 ); /* IE6-9 */
	
	position: relative;
}

.healthcourse-record-icon {
	float: left;
}

.healthcourse-record-info {
	float: left;
	padding-left: 10px;
}

.healthcourse-record-title {
	position: absolute;
	left: 100px;
}
.healthcourse-record-perf {
  position: absolute;
  left: 100px;
  top: 25px;
}
.healthcourse-record-status {
  position: absolute;
  left: 100px;
  top: 60px;
  font-size: 11px;
}

.healthcourse-record-goto {
	position: absolute;
	bottom: 4px;
	right: 4px;
}

.healthcourse-record-cert {
	position: absolute;
	bottom: 4px;
	right: 204px;
}

.healthcourse-record-pdf {
  position: absolute;
  bottom: 4px;
  right: 414px;
}

