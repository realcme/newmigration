<script language="JavaScript">
<!--
<!-- Set min and max time to display a single banner ad -->
var min_delay_range = 15;
var max_delay_range = 20;

<!-- Declare global variables -->
var banner1_image;
var banner2_image;
var banner3_image;
var banner4_image;
var banner5_image;
var banner6_image;
var banner1_rollover;
var banner2_rollover;
var banner3_rollover;
var banner4_rollover;
var banner5_rollover;
var banner6_rollover;
var banner1_link;
var banner2_link;
var banner3_link;
var banner4_link;
var banner5_link;
var banner6_link;
var banner1_target;
var banner2_target;
var banner3_target;
var banner4_target;
var banner5_target;
var banner6_target;
var do_rotate;

<!-- Generate unique random numbers (for use when image_list.length < banner_count) -->
var rand_num1 = generate(0, 2);
var rand_num2 = generate(0, 2);
while (rand_num1 == rand_num2) {
  rand_num2 = generate(0, 2);
}

<!-- Set array of images -->
var image_index = 0;
image_list = new Array();

<!-- Set array of image rollovers -->
var rollover_index = 0;
rollover_list = new Array();

<!-- Set array of image URLs -->
var url_index = 0;
url_list = new Array();

<!-- Set array of target windows -->
var target_index = 0;
target_list = new Array();

<!-- Set array of placement for required images -->
placement_list = new Array();
placement_list[0] = -1;
placement_list[1] = -1;
placement_list[2] = -1;
placement_list[3] = -1;
placement_list[4] = -1;
placement_list[5] = -1;

function imageItem(image_location) {
  this.image_item = new Image();
  this.image_item.src = image_location;
}

function get_ImageItemLocation(imageObj) {
  return(imageObj.image_item.src)
}

function generate(x, y) {
  var range = y - x + 1;
  return Math.floor(Math.random() * range) + x;
}

function getNextImage() {
  image_index = generate(0, image_list.length-1);
  var new_image = get_ImageItemLocation(new imageItem(image_list[image_index]));
  return(new_image);
}

<!-- Main function called on page load -->
function rotateImage(banner,link) {
  <!-- Check to make sure banner ads appear on the page before proceeding -->
  var top_banners_exist = "N";
  var bot_banners_exist = "N";
  var banner_count = 0;
  for (i=0; i<document.images.length; i++) {
    if (document.images[i].name == "banner3") {
      top_banners_exist = "Y";
      banner_count = banner_count + 3;
    }
    if (document.images[i].name == "banner6") {
      bot_banners_exist = "Y";
      banner_count = banner_count + 3;
    }
  }

  <!-- If there are not more images available than ad slots -->
  if (banner_count >= image_list.length) {
    if (image_list.length >= 3) {
      if (top_banners_exist == "Y") {
        document["banner1"].src = get_ImageItemLocation(new imageItem(image_list[0]));
        banner1_image = image_list[0];
        banner1_rollover = rollover_list[0];
        banner1_link = url_list[0];
        banner1_target = target_list[0];
        document["banner2"].src = get_ImageItemLocation(new imageItem(image_list[1]));
        banner2_image = image_list[1];
        banner2_rollover = rollover_list[1];
        banner2_link = url_list[1];
        banner2_target = target_list[1];
        document["banner3"].src = get_ImageItemLocation(new imageItem(image_list[2]));
        banner3_image = image_list[2];
        banner3_rollover = rollover_list[2];
        banner3_link = url_list[2];
        banner3_target = target_list[2];
      }
    }
    if (bot_banners_exist == "Y") {
      if (image_list.length == 3) {
        document["banner4"].src = get_ImageItemLocation(new imageItem(image_list[0]));
        banner4_image = image_list[0];
        banner4_rollover = rollover_list[0];
        banner4_link = url_list[0];
        banner4_target = target_list[0];
        document["banner5"].src = get_ImageItemLocation(new imageItem(image_list[1]));
        banner5_image = image_list[1];
        banner5_rollover = rollover_list[1];
        banner5_link = url_list[1];
        banner5_target = target_list[1];
        document["banner6"].src = get_ImageItemLocation(new imageItem(image_list[2]));
        banner6_image = image_list[2];
        banner6_rollover = rollover_list[2];
        banner6_link = url_list[2];
        banner6_target = target_list[2];
      }
      else if (image_list.length == 4) {
        document["banner4"].src = get_ImageItemLocation(new imageItem(image_list[3]));
        banner4_image = image_list[3];
        banner4_rollover = rollover_list[3];
        banner4_link = url_list[3];
        banner4_target = target_list[3];
        document["banner5"].src = get_ImageItemLocation(new imageItem(image_list[rand_num1]));
        banner5_image = image_list[rand_num1];
        banner5_rollover = rollover_list[rand_num1];
        banner5_link = url_list[rand_num1];
        banner5_target = target_list[rand_num1];
        document["banner6"].src = get_ImageItemLocation(new imageItem(image_list[rand_num2]));
        banner6_image = image_list[rand_num2];
        banner6_rollover = rollover_list[rand_num2];
        banner6_link = url_list[rand_num2];
        banner6_target = target_list[rand_num2];
      }
      else if (image_list.length == 5) {
        document["banner4"].src = get_ImageItemLocation(new imageItem(image_list[3]));
        banner4_image = image_list[3];
        banner4_rollover = rollover_list[3];
        banner4_link = url_list[3];
        banner4_target = target_list[3];
        document["banner5"].src = get_ImageItemLocation(new imageItem(image_list[4]));
        banner5_image = image_list[4];
        banner5_rollover = rollover_list[4];
        banner5_link = url_list[4];
        banner5_target = target_list[4];
        document["banner6"].src = get_ImageItemLocation(new imageItem(image_list[rand_num1]));
        banner6_image = image_list[rand_num1];
        banner6_rollover = rollover_list[rand_num1];
        banner6_link = url_list[rand_num1];
        banner6_target = target_list[rand_num1];
      }
      else if (image_list.length == 6) {
        document["banner4"].src = get_ImageItemLocation(new imageItem(image_list[3]));
        banner4_image = image_list[3];
        banner4_rollover = rollover_list[3];
        banner4_link = url_list[3];
        banner4_target = target_list[3];
        document["banner5"].src = get_ImageItemLocation(new imageItem(image_list[4]));
        banner5_image = image_list[4];
        banner5_rollover = rollover_list[4];
        banner5_link = url_list[4];
        banner5_target = target_list[4];
        document["banner6"].src = get_ImageItemLocation(new imageItem(image_list[5]));
        banner6_image = image_list[5];
        banner6_rollover = rollover_list[5];
        banner6_link = url_list[5];
        banner6_target = target_list[5];
      }
    }
  }

  <!-- Randomly create rotating banner ads --->
  else if (banner_count < image_list.length) {
    <!-- Display required images --->
    if ((banner == "banner1") && (placement_list[0] > -1)) {
      document["banner1"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[0]]));
      banner1_image = image_list[placement_list[0]];
      banner1_rollover = rollover_list[placement_list[0]];
      banner1_link = url_list[placement_list[0]];
      banner1_target = target_list[placement_list[0]];
      do_rotate = "N";
    }
    else if ((banner == "banner2") && (placement_list[1] > -1)) {
      document["banner2"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[1]]));
      banner2_image = image_list[placement_list[1]];
      banner2_rollover = rollover_list[placement_list[1]];
      banner2_link = url_list[placement_list[1]];
      banner2_target = target_list[placement_list[1]];
      do_rotate = "N";
    }
    else if ((banner == "banner3") && (placement_list[2] > -1)) {
      document["banner3"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[2]]));
      banner3_image = image_list[placement_list[2]];
      banner3_rollover = rollover_list[placement_list[2]];
      banner3_link = url_list[placement_list[2]];
      banner3_target = target_list[placement_list[2]];
      do_rotate = "N";
    }
    else if ((banner == "banner4") && (placement_list[3] > -1)) {
      document["banner4"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[3]]));
      banner4_image = image_list[placement_list[3]];
      banner4_rollover = rollover_list[placement_list[3]];
      banner4_link = url_list[placement_list[3]];
      banner4_target = target_list[placement_list[3]];
      do_rotate = "N";
    }
    else if ((banner == "banner5") && (placement_list[4] > -1)) {
      document["banner5"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[4]]));
      banner5_image = image_list[placement_list[4]];
      banner5_rollover = rollover_list[placement_list[4]];
      banner5_link = url_list[placement_list[4]];
      banner5_target = target_list[placement_list[4]];
      do_rotate = "N";
    }
    else if ((banner == "banner6") && (placement_list[5] > -1)) {
      document["banner6"].src = get_ImageItemLocation(new imageItem(image_list[placement_list[5]]));
      banner6_image = image_list[placement_list[5]];
      banner6_rollover = rollover_list[placement_list[5]];
      banner6_link = url_list[placement_list[5]];
      banner6_target = target_list[placement_list[5]];
      do_rotate = "N";
    }
    else {
      <!-- If not required, then randomly get new image from array -->
      var new_image = getNextImage();
      do_rotate = "Y";
    }

    if (do_rotate == "Y") {
      <!-- Check if the selected image is already being displayed -->
      var image_displayed = "N";
      if (top_banners_exist == "Y") {
        if ((new_image == document["banner1"].src) ||
            (new_image == document["banner2"].src) ||
            (new_image == document["banner3"].src)) {
          image_displayed = "Y";
        }
      }
      if (bot_banners_exist == "Y") {
        if ((new_image == document["banner4"].src) ||
            (new_image == document["banner5"].src) ||
            (new_image == document["banner6"].src)) {
          image_displayed = "Y";
        }
      }

      <!-- If the selected image is not currently dsiplayed then -->
      if (image_displayed == "N") {
        <!-- Set the new images properties -->
        if (banner == "banner1") {
          banner1_image = image_list[image_index];
          banner1_rollover = rollover_list[image_index];
          banner1_link = url_list[image_index];
          banner1_target = target_list[image_index];
         }
        else if (banner == "banner2") {
          banner2_image = image_list[image_index];
          banner2_rollover = rollover_list[image_index];
          banner2_link = url_list[image_index];
          banner2_target = target_list[image_index];
          }
         else if (banner == "banner3") {
          banner3_image = image_list[image_index];
          banner3_rollover = rollover_list[image_index];
          banner3_link = url_list[image_index];
          banner3_target = target_list[image_index];
        }
        else if (banner == "banner4") {
          banner4_image = image_list[image_index];
          banner4_rollover = rollover_list[image_index];
          banner4_link = url_list[image_index];
          banner4_target = target_list[image_index];
          }
        else if (banner == "banner5") {
          banner5_image = image_list[image_index];
          banner5_rollover = rollover_list[image_index];
          banner5_link = url_list[image_index];
          banner5_target = target_list[image_index];
        }
        else if (banner == "banner6") {
          banner6_image = image_list[image_index];
          banner6_rollover = rollover_list[image_index];
          banner6_link = url_list[image_index];
          banner6_target = target_list[image_index];
        }

        <!-- Display the new image -->
        document[banner].src = new_image;

        <!-- Set the timeout period for the new image  -->
        var recur_call = "rotateImage('"+banner+"','"+link+"')";
        var interval = (Math.floor(Math.random()*(max_delay_range-min_delay_range))+min_delay_range)*1000;
        setTimeout(recur_call, interval);
      }

      <!-- If the selected image is already being displayed then -->
      else {
        <!-- Continually call this function until a suitable image is selected -->
        rotateImage(banner,link)
      }

    }
  }
}

function launch(banner, banner_url) {
  var popup = false;
  if (banner == "banner1") {
    if (banner1_target == "_blank") popup = true;
  }
  else if (banner == "banner2") {
    if (banner2_target == "_blank") popup = true;
  }
  else if (banner == "banner3") {
    if (banner3_target == "_blank") popup = true;
  }
  else if (banner == "banner4") {
    if (banner4_target == "_blank") popup = true;
  }
  else if (banner == "banner5") {
    if (banner5_target == "_blank") popup = true;
  }
  else if (banner == "banner6") {
    if (banner6_target == "_blank") popup = true;
  }
  if (popup) {
    window.open(banner_url,'','');
  }
  else {
    location.href=banner_url;
  }
}
//-->
</script>

